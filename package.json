{
	"name": "dafunda-blocks",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"unlock-limit": "echo fs.inotify.max_user_watches=582222 | sudo tee -a /etc/sysctl.conf && sudo sysctl -p",
		"start": "concurrently -k \"wp-scripts start\" \"node recompileOnChange.js\" \"npx tailwindcss -i ./assets/blocks.style.css -o ./dist/blocks.style.build.css -c ./tailwind.config.js --minify --watch\" \"npx tailwindcss -i ./assets/blocks.editor.css -o ./dist/blocks.editor.build.css -c tailwind.config.js --minify --watch\"",
		"prebuild": "pnpm clean-dist && npx tailwindcss -i ./assets/blocks.style.css -o ./dist/blocks.style.build.css -c ./tailwind.config.js --minify && npx tailwindcss -i ./assets/blocks.editor.css -o ./dist/blocks.editor.build.css -c ./tailwind.config.js --minify",
		"build": "wp-scripts build & node recompile.js",
		"format": "prettier -w .",
		"check": "prettier -c .",
		"clean": "rimraf {node_modules, dist}",
		"clean-dist": "rimraf dist",
		"prepare": "husky install",
		"commit": "git-cz",
		"prettier": "prettier"
	},
	"dependencies": {
		"@babel/core": "^7.18.2",
		"@babel/plugin-proposal-class-properties": "^7.17.12",
		"@babel/preset-env": "^7.18.2",
		"@babel/preset-react": "^7.17.12",
		"@fortawesome/fontawesome-svg-core": "^6.1.1",
		"@fortawesome/free-brands-svg-icons": "^6.1.1",
		"@fortawesome/free-solid-svg-icons": "^6.1.1",
		"@fortawesome/react-fontawesome": "^0.1.18",
		"clean-webpack-plugin": "^4.0.0",
		"moment": "^2.29.3",
		"prop-types": "^15.8.1",
		"react": "^18.1.0",
		"react-dom": "^18.1.0",
		"react-sortable-hoc": "^2.0.0",
		"react-structured-data": "0.0.14",
		"swiper": "^8.1.5"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.0.2",
		"@commitlint/config-conventional": "^17.0.2",
		"@commitlint/cz-commitlint": "^17.0.0",
		"@prettier/plugin-php": "^0.18.7",
		"@svgr/webpack": "^6.2.1",
		"@tailwindcss/forms": "^0.5.2",
		"@tailwindcss/line-clamp": "^0.4.0",
		"@wordpress/scripts": "^23.2.0",
		"autoprefixer": "^10.4.7",
		"babel-loader": "^8.2.5",
		"commitizen": "^4.2.4",
		"concurrently": "^7.2.1",
		"css-loader": "^6.7.1",
		"gulp": "^4.0.2",
		"gulp-notify": "^4.0.0",
		"gulp-sort": "^2.0.0",
		"gulp-wp-pot": "^2.5.0",
		"husky": "^8.0.1",
		"ignore-emit-webpack-plugin": "^2.0.6",
		"lint-staged": "^13.0.0",
		"mini-css-extract-plugin": "^2.6.0",
		"path": "^0.12.7",
		"postcss": "^8.4.14",
		"postcss-loader": "^7.0.0",
		"postcss-preset-env": "^7.7.0",
		"prettier": "^2.6.2",
		"rimraf": "^3.0.2",
		"tailwindcss": "^3.0.24",
		"url-loader": "^4.1.1"
	},
	"lint-staged": {
		"**/*.{js,jsx,ts,tsx,html,css,json}": [
			"npx prettier --write"
		]
	},
	"config": {
		"commitizen": {
			"path": "@commitlint/cz-commitlint"
		}
	}
}
